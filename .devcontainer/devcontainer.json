{
  "name": "AI50 Parser Environment",
  "image": "ghcr.io/cs50/ai50:latest",
  "features": {
    "ghcr.io/devcontainers/features/python:1": {},
    "ghcr.io/devcontainers/features/git:1": {}
  },
  "postCreateCommand": "pip install check50 submit50",
  "customizations": {
    "codespaces": {
      "openFiles": ["README.md"]
    },
    "vscode": {
      "extensions": [
        "cs50.cs50",
        "ms-python.python"
      ]
    }
  }
}
